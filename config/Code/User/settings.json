{
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "vim.disableAnnoyingNeovimMessage": true,
  "window.zoomLevel": 0,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "golang.Go",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Solarized Dark",
  "[yaml]": {},
  "yaml.schemas": {
    "Kubernetes": "*.yaml"
  },
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "editor.tabCompletion": "on",
  "emmet.triggerExpansionOnTab": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["K"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["J"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<C-w>"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["t"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.goToReferences"]
    }
  ],
  "prettier.singleQuote": true,
  "tabnine.experimentalAutoImports": true,
  "jenkins.pipeline.linter.connector.crumbUrl": "https://dp-jenkins.dot-c.daimler.com/crumbIssuer/api/xml?xpath=concat(//crumbRequestField,%22:%22,//crumb))",
  "jenkins.pipeline.linter.connector.url": "https://dp-jenkins.dot-c.app.corpintra.net/pipeline-model-converter/validate",
  "jenkins.pipeline.linter.connector.token": "116255a3dad3403b74f510faef0a7e2acb",
  "jenkins.pipeline.linter.connector.strictssl": false,
  "prettier.printWidth": 160,
  "redhat.telemetry.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path.linux": "/lhome/dkadiog/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vscode-kubernetes.helm-path.mac": "/Users/dkadiog/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path.mac": "/Users/dkadiog/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vs-kubernetes.minikube-show-information-expiration": "2023-12-27T15:18:20.398Z"
  },
  "git.autofetch": true,
  "security.workspace.trust.untrustedFiles": "open",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "go.toolsManagement.autoUpdate": true,
  "bridgeToKubernetes.disconnectAfterDebugging": false,
  "vscode-kubernetes.helm-path": "/opt/homebrew/bin/helm",
  "vscode-kubernetes.helm-path.mac": "/opt/homebrew/bin/helm",
  "vscode-kubernetes.helm-path.linux": "undefined/opt/homebrew/bin/helm",
  "vscode-kubernetes.helm-path.windows": "/opt/homebrew/bin/helm",
  "vscode-kubernetes.kubectl-path": "/opt/homebrew/bin/kubectl",
  "vscode-kubernetes.kubectl-path.linux": "/opt/homebrew/bin/kubectl",
  "vscode-kubernetes.kubectl-path.mac": "/opt/homebrew/bin/kubectl",
  "vscode-kubernetes.kubectl-path.windows": "/opt/homebrew/bin/kubectl",
  "editor.inlineSuggest.enabled": true
}
